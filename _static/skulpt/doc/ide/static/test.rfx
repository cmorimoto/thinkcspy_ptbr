      e = vec3(0,4,0);
      er=sin(t*.5)+3.02;
      a1=sin(t)*.1-.9;
      a2=sin(t*.2)*2.;
      a3=sin(t*.7)*.1;
   }
   else if (t <= 64.0 || (t > 72.0 && t <= 96.0))
   {
