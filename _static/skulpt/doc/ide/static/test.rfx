   if(t>=92.&&t<96.)
      c*=1.-pow(min((t-92.)/4.,1.),2.);
   if(t>=96.)
      c*=min((t-96.)/5.,1.);
   if(t>=140.)
      c*=1.-min((t-140.)/8.,1.);
   c*=1.-.85*length(gl_FragCoord.xy-vec2(width2,height2))/height;
   gl_FragColor=vec4(c,1);
}


