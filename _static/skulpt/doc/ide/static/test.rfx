      d=ooo(p);
      if(d<=0.001)
      {
         vec3 l=normalize(lv-p+e);
         n=normalize(vec3(ooo(p+vec3(.01,0,0))-d,ooo(p+vec3(0,.01,0))-d,ooo(p+vec3(0,0,.01))-d))+b;
         r=reflect(l,n);
         if(cm==1.)
            c*=g;
         c2=((1.-g)*k*h*(max(dot(n,l),0.)+pow(max(dot(r,v),0.),17.)))*cm;
         for(i=1.;i<5.;i++)
            c2-=vec3((i*.2-ooo(p+n*i*.2))/pow(2.,i));
         c+=max(c2,0.);
         if (rr>0.&&w++<1.)
         {
            cm*=.5;
            p-=v*(d+.2);
            v=reflect(v,n);
         }
         else
         {
            break;
         }
      }
      p+=v*max(d,.002);
   }
